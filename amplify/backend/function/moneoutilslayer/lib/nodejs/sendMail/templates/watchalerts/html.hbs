<!doctype html>
<html>

<head>
	<meta name="viewport" content="width=device-width" />
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
	<title>Moneo Buy / Sell Alerts</title>
</head>

<body>
	{{#> layout templateName="watchalerts" }}

	<div>
		<p class="text-title ">Buy / Sell Alerts</p>
	</div>

	{{#if buy}}
	<div class="box-container-high">
		<p class='main-text'>{{buy.length}} buy alerts</p>
		{{#each buy}}
		<div class="data-container">
			<a>{{this.name}}</a>
		</div>
		{{/each}}
	</div>
	{{/if}}

	{{#if sell}}
	<div class="box-container-low">
		<p class='main-text'>{{sell.length}} sell alerts</p>
		{{#each sell}}
		<div class="data-container">
			<a>{{this.name}}</a>
		</div>
		{{/each}}
	</div>
	{{/if}}

	<table role="presentation" border="0" cellpadding="0" cellspacing="0" class="btn btn-primary">
		<tbody>
			<tr>
				<td align="center">
					<a href="https://moneo.in/dashboard" target="_blank">
						View Details
				</td>
			</tr>
		</tbody>
	</table>
	{{/layout}}
</body>

</html>